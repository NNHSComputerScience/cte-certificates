
<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->


    <!-- Main Site -->
    <main id="site-main">
        <div class="container">

            <% for(var certificate of certificates) { %>
                <div class="certificate">
                    <input type="checkbox" class="certificate_checkbox" id="<%= certificate.name %>">
                            <label for="<%= certificate.name %>"><%= certificate.name %></label>
                </div>
        
            <% } %>
        </div>

        <input type="button" value="Submit" onclick="submitCertificates()">

    </main>
<!-- /Main Site -->


